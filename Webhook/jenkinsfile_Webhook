pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'python3  Webhook/main.py'
            }
        }
        stage('deploy'){
        sh 'echo Deploy build'
        }
            stage('release'){
        sh 'echo Release'
        }
        
    }
    post{
    always{
    echo 'Always'
    }
    success{
    echo 'success'
    }
    failure{
    echo 'failure'
    }
    
    
    }
}
